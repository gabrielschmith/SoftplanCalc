{
  "Serilog": {
    "Using": [ "Serilog.Sinks.ColoredConsole" ],
    "MinimumLevel": "Verbose",
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "SoftplanCalc.Api"
    }
  },
  "AllowedHosts": "*",
  "Github": "https://github.com/gabrielschmith/SoftplanCalc",
  "CalculateInterest": {
    "InterestRate": 0.01
  }
}
